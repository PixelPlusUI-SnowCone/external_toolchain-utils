[
  {
    "wd": "/tmp/stable",
    "env": [
      "BISECT_STAGE=someBisectStage",
      "BISECT_DIR=someBisectDir"
    ],
    "wrapper": {
      "cmd": {
        "path": "./x86_64-cros-linux-gnu-clang",
        "args": [
          "main.cc"
        ]
      }
    },
    "cmds": [
      {
        "cmd": {
          "path": "/usr/bin/python2",
          "args": [
            "-c",
            "import bisect_driver; sys.exit(bisect_driver.bisect_driver(sys.argv[1], sys.argv[2], sys.argv[3:]))",
            "someBisectStage",
            "someBisectDir",
            "/usr/bin/ccache",
            "../../usr/bin/clang",
            "--sysroot=/usr/x86_64-cros-linux-gnu",
            "-Qunused-arguments",
            "-Wno-tautological-constant-compare",
            "-Wno-tautological-unsigned-enum-zero-compare",
            "-Wno-unknown-warning-option",
            "-Wno-section",
            "-static-libgcc",
            "main.cc",
            "-B../../bin",
            "-target",
            "x86_64-cros-linux-gnu"
          ],
          "env_updates": [
            "CCACHE_BASEDIR=/usr/x86_64-cros-linux-gnu",
            "CCACHE_DIR=/var/cache/distfiles/ccache",
            "CCACHE_UMASK=002",
            "CCACHE_CPP2=yes"
          ]
        }
      }
    ]
  },
  {
    "wd": "/tmp/stable",
    "env": [
      "BISECT_STAGE=someBisectStage",
      "BISECT_DIR=someBisectDir"
    ],
    "wrapper": {
      "cmd": {
        "path": "./x86_64-cros-linux-gnu-clang",
        "args": [
          "main.cc"
        ]
      },
      "stdout": "somemessage",
      "stderr": "someerror",
      "exitcode": 1
    },
    "cmds": [
      {
        "cmd": {
          "path": "/usr/bin/python2",
          "args": [
            "-c",
            "import bisect_driver; sys.exit(bisect_driver.bisect_driver(sys.argv[1], sys.argv[2], sys.argv[3:]))",
            "someBisectStage",
            "someBisectDir",
            "/usr/bin/ccache",
            "../../usr/bin/clang",
            "--sysroot=/usr/x86_64-cros-linux-gnu",
            "-Qunused-arguments",
            "-Wno-tautological-constant-compare",
            "-Wno-tautological-unsigned-enum-zero-compare",
            "-Wno-unknown-warning-option",
            "-Wno-section",
            "-static-libgcc",
            "main.cc",
            "-B../../bin",
